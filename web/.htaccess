RewriteEngine on
# Si un directorio o fichero existe, se utiliza directamente
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
# Sino, se env√≠a a index.php
RewriteRule . index.php
